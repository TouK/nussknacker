    "build": "NODE_ENV=production webpack --progress && cp -R assets/icons/license dist",
    "check-types": "tsc",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx",
    "svgo": "svgo --config .svgo.yml",
    "check": "npm run check-types && npm run lint -- . --rule 'i18next/no-literal-string: warn'",
    "start": "BACKEND_DOMAIN=http://localhost:8080 webpack serve",
    "start-backend-staging": "BACKEND_DOMAIN=https://staging.nussknacker.io webpack serve",
    "start-backend-demo": "BACKEND_DOMAIN=https://demo.nussknacker.io webpack serve",
    "pretest": "npm run check",
    "test": "npm run test:unit && npm run test:e2e:ci",
    "test:unit": "jest",
    "test:e2e": "cypress run",
    "test:e2e:ci": "start-server-and-test start http-get://localhost:3000/static/main.html test:e2e",
    "test:e2e:docker": "docker run -it -v $PWD:/client -w /client cypress/included:6.5.0 --config baseUrl=http://host.docker.internal:3000",
    "test:e2e:dev": "cypress open",
    "coverage": "npx merge-cypress-jest-coverage"

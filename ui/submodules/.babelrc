{
    "presets": ["@babel/preset-env", "@babel/preset-typescript", "@babel/preset-react"],
    "plugins": [
        "@babel/plugin-transform-runtime",
        [
            "babel-plugin-direct-import",
            {
                "modules": ["@mui/material", "@mui/icons-material"]
            }
        ]
    ],
    "env": {
        "development": {
            "plugins": ["react-refresh/babel"]
        }
    }
}

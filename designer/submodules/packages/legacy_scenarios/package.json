  "@comment lint-staged": "We add svgo preprocessing as a pre-commit hook to avoid surprise that someone based on global style classes from svgo that won't be available anymore after preprocessing",
  "lint-staged": {
    "**/*.(js|ts)?(x)": [
      "npm run lint -- --rule 'i18next/no-literal-string: warn' --fix",
      "git add"
    ],
    "**/*.svg": [
      "npm run svgo",
      "git add"
    ]
  },
  "private": true,
  "scripts": {
    "build": "NODE_ENV=production webpack",
    "check": "npm run lint -- . --rule 'i18next/no-literal-string: warn'",
    "clean-translations": "rm -rf ./translations/*",
    "lint": "eslint --fix .",
    "make-translations": "i18next 'src/**/*.{js,ts,tsx}'",
    "make-types": "make-federated-types",
    "prebuild": "npm run make-translations",
    "prestart": "npm run clean-translations",
    "pretest": "npm run check",
    "start": "PORT=5002 webpack serve",
    "svgo": "svgo --folder src/assets --recursive",
    "test": "jest --reporters=default --reporters=jest-junit"
  }
}
